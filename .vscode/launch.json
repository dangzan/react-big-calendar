{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Storybook in Firefox",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:9003",
      "webRoot": "${workspaceFolder}",
      "pathMappings": [
        {
          "url": "webpack://react-big-calendar/src",
          "path": "${workspaceFolder}/src"
        },
        {
          "url": "webpack:///src/",
          "path": "${webRoot}/src/"
        }
      ]
    }
  ]
}
